{
    "defaultBranch": "main",
    "branches": {
        "main": {
            "proxy": {
                "engine": "velocity",
                "mounts": {
                    "luckperms-libs": {
                        "src": "plugins/LuckPerms/libs",
                        "dest": "plugins/luckperms/libs"
                    }
                },
                "plugins": {
                    "Managed": {
                        "floodgate": {
                            "pattern": "floodgate*"
                        },
                        "geyser": {
                            "pattern": "geyser*"
                        },
                        "luckperms": {
                            "pattern": "LuckPerms-Velocity-*"
                        },
                        "viaversion": {
                            "pattern": "ViaVersion-*"
                        },
                        "cmiv": {
                            "pattern": "cmiv*"
                        }
                    },
                    "Manual": {
                        "litbans": {
                            "pattern": "LiteBans*"
                        }
                    }
                }
            },
            "wildy": {
                "engine": "paper",
                "mounts": {
                    "paper-cache": {
                        "src": "cache",
                        "dest": "cache"
                    },
                    "paper-libs": {
                        "src": "libraries",
                        "dest": "libraries"
                    },
                    "paper-versions": {
                        "src": "versions",
                        "dest": "versions"
                    },
                    "luckperms-libs": {
                        "src": "plugins/LuckPerms/libs",
                        "dest": "plugins/LuckPerms/libs"
                    }
                },
                "plugins": {
                    "Managed": {
                        "CMILib": {
                            "pattern": "CMILib*"
                        },
                        "Chunky": {
                            "pattern": "Chunky-*"
                        },
                        "ChunkyBorder": {
                            "pattern": "ChunkyBorder-*"
                        },
                        "DiscordSRV": {
                            "pattern": "DiscordSRV-*"
                        },
                        "FastAsyncWorldEdit": {
                            "pattern": "FastAsyncWorldEdit-Paper-*"
                        },
                        "LuckPerms": {
                            "pattern": "LuckPerms-*"
                        },
                        "PlaceholderAPI": {
                            "pattern": "PlaceholderAPI-*"
                        },
                        "ProtocolLib": {
                            "pattern": "ProtocolLib*"
                        },
                        "TAB": {
                            "pattern": "TAB*"
                        },
                        "WorldGuard": {
                            "pattern": "worldguard-bukkit-*"
                        },
                        "GriefPrevention": {
                            "pattern": "GriefPrevention*"
                        },
                        "NuVotifier": {
                            "pattern": "NuVotifier*"
                        },
                        "Vault": {
                            "pattern": "Vault*"
                        },
                        "mcMMO": {
                            "pattern": "mcMMO*"
                        }
                    },
                    "Manual": {
                        "CMI": {
                            "pattern": "CMI-*"
                        },
                        "MobFarmManager": {
                            "pattern": "MobFarmManager*"
                        },
                        "CoreProtect": {
                            "pattern": "CoreProtect-*"
                        }
                    }
                }
            }
        },
        "dev": {
            "parent": "main"
        }
    },
    "definitions": {
        "paths": {
            "Managed": "libraries/plugins/{engine}/Managed",
            "Manual": "libraries/plugins/{engine}"
        }
    }
}