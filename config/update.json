  "strategies": {
    "Direct": {
      "type": "static",
      "description": "Direct URL download",
      "download_template": "{url}"
    },
    "Jenkins": {
      "type": "api",
      "description": "Standard Jenkins Artifacts",
      "api_template": "https://{host}/job/{project}/lastSuccessfulBuild/api/json",
      "artifact_filter": ".artifacts[].relativePath",
      "download_template": "https://{host}/job/{project}/lastSuccessfulBuild/artifact/{artifact}",
      "priority": [
        "{platform}",
        "universal",
        "all",
        "\\.jar$"
      ]
    },
    "Github": {
      "type": "api",
      "description": "GitHub Latest Release",
      "api_template": "https://api.github.com/repos/{owner}/{repo}/releases/latest",
      "artifact_filter": ".assets[].browser_download_url",
      "download_template": "{artifact}",
      "priority": [
        "{platform}",
        "paper",
        "spigot",
        "\\.jar$"
      ]
    },
    "EngineHub": {
      "type": "web-scrape",
      "description": "Scrapes EngineHub file list (Specific Implementation)",
      "api_template": "https://ci.enginehub.org/job/{project}/lastSuccessfulBuild/api/json",
      "artifact_filter": ".artifacts[].relativePath",
      "download_template": "https://ci.enginehub.org/job/{project}/lastSuccessfulBuild/artifact/{artifact}",
      "priority": [
        "{platform}",
        "\\.jar$"
      ]
    },
    "Zrips": {
      "type": "web-scrape",
      "description": "Scrapes Zrips file list (Specific Implementation)",
      "download_template": "https://www.zrips.net/{project}"
    },
    "Manual": {
      "type": "manual",
      "description": "Requires browser interaction or external auth",
      "download_template": "{url}"
    }
  }
}